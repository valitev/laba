#include <stdio.h>
#include <math.h>

int main() {
    double G, F, Y, x, a;
    scanf("%f", &x);
    scanf("%f", &a);

    G = 4*((2*(-10*a*a+7*a*x+6*x*x)/(-9*a*a+10*a*x+16*x*x)));
    F = cos(25*a*a-50*a*x+9*x*x);
    Y = acos(21*a*a+59*a*x+8*x*x+1);

    printf("%0.8f\n", G);
    printf("%0.8f\n", F);
    printf("%0.8f\n", Y);

    return 0;
}
